{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/design/assets/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/design/assets",
        "tsConfig": "packages/design/assets/tsconfig.lib.json",
        "project": "packages/design/assets/package.json",
        "entryFile": "packages/design/assets/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/design/assets/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/design/assets/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/design/assets"],
      "options": {
        "jestConfig": "packages/design/assets/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint-test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx lint design-assets", "nx test design-assets"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "design-assets@",
        "postTargets": ["design-assets:build", "design-assets:publish"]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy"
    }
  }
}
