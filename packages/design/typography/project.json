{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/design/typography/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/design/typography",
        "tsConfig": "packages/design/typography/tsconfig.lib.json",
        "project": "packages/design/typography/package.json",
        "entryFile": "packages/design/typography/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/design/typography/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/design/typography/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/design/typography"],
      "options": {
        "jestConfig": "packages/design/typography/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint-test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx lint design-typography", "nx test design-typography"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "design-typography@",
        "postTargets": ["design-typography:build", "design-typography:publish"]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy"
    }
  }
}
