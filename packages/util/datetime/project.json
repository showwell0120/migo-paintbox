{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/util/datetime/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/util/datetime",
        "main": "packages/util/datetime/src/index.ts",
        "tsConfig": "packages/util/datetime/tsconfig.lib.json",
        "assets": ["packages/util/datetime/*.md"]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/util/datetime/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/util/datetime"],
      "options": {
        "jestConfig": "packages/util/datetime/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint-test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx lint util-datetime", "nx test util-datetime"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "util-datetime@",
        "postTargets": ["util-datetime:build", "util-datetime:publish"]
      }
    }
  },
  "tags": []
}
