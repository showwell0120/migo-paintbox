{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/model/video/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/model/video",
        "main": "packages/model/video/src/index.ts",
        "tsConfig": "packages/model/video/tsconfig.lib.json",
        "assets": ["packages/model/video/*.md"]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/model/video/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/model/video"],
      "options": {
        "jestConfig": "packages/model/video/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint-test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx lint model-video", "nx test model-video"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "model-video@",
        "postTargets": ["model-video:build", "model-video:publish"]
      }
    }
  },
  "tags": []
}
